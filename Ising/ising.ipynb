    "ax[1, 2].set_title('System Energy vs Temp') ,
    "# ax[1, 0].set_title('Magnetization') ,
    " ,
    " ,
    "line_ani = animation.FuncAnimation(fig, animate,500,repeat=False ) ,
    " ,
    "plt.show() ,
    " ,
    "
